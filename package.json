{
  "private": true,
  "name": "trkw",
  "homepage": "https://trkw.me",
  "twitter": "trkw_",
  "main": "main.js",
  "engines": {
    "node": "5.10.0",
    "npm": "3.8.3"
  },
  "repository": "https://github.com/trkw/trkw",
  "scripts": {
    "build": "phenomic build",
    "deploy": "./deploy.sh",
    "lint": "npm-run-all --parallel lint:*",
    "lint:css": "stylelint \"web_modules/**/*.css\"",
    "lint:js": "eslint --ignore-path .gitignore --fix .",
    "pretest": "npm run lint",
    "start": "phenomic start",
    "test": "npm run build"
  },
  "phenomic": {
    "CNAME": true
  },
  "config": {
    "googleAnalyticsUA": "UA-65606612-3"
  },
  "babel": {
    "env": {
      "production": {
        "presets": [
          "babel-preset-react-optimize"
        ]
      }
    },
    "presets": [
      "babel-preset-es2015",
      "babel-preset-react",
      "babel-preset-stage-1"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-i-am-meticulous/react"
    ],
    "parser": "babel-eslint",
    "rules": {
      "react/prefer-stateless-function": 0
    }
  },
  "eslintConfigRuleReact/prefer-stateless-function": "https://github.com/MoOx/phenomic/issues/46",
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "dependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^6.0.0-beta.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-1": "^6.3.13",
    "css-loader": "^0.23.0",
    "eslint": "^3.6.0",
    "eslint-config-i-am-meticulous": "^4.1.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^6.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "history": "^4.3.0",
    "invariant": "^2.1.1",
    "json-loader": "^0.5.2",
    "material-design-lite": "^1.2.0",
    "moment": "^2.14.1",
    "npm-run-all": "^3.1.0",
    "p5": "^0.5.3",
    "phenomic": "^0.16.2",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-loader": "^0.7.0",
    "postcss-nested": "^1.0.0",
    "postcss-reporter": "^1.3.0",
    "raw-loader": "^0.5.1",
    "react": "^0.14.0 || ^15.3.1",
    "react-dom": "^0.14.0 || ^15.3.1",
    "react-ga": "^2.1.1",
    "react-helmet": "^3.0.0",
    "react-icons": "^2.2.1",
    "react-redux": "^4.0.0",
    "react-router": "^2.3.0",
    "react-social-sharebuttons": "trkw/react-social-sharebuttons",
    "react-svg-inline": "^1.1.0",
    "react-topbar-progress-indicator": "^1.0.0",
    "redux": "^3.0.0",
    "style-loader": "^0.12.3",
    "stylelint": "^6.8.0",
    "stylelint-config-standard": "^10.0.0",
    "webpack": "^1.12.1",
    "whatwg-fetch": "^1.0.0"
  }
}
