{
  "private": true,
  "name": "trkw",
  "homepage": "https://trkw.me/",
  "googleAnalyticsUA": "UA-65606612-3",
  "twitter": "trkw_",
  "repository": "https://github.com/trkw/trkw.git",
  "scripts": {
    "build": "phenomic build",
    "lint": "npm-run-all --parallel lint:*",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:js": "eslint --ignore-path .gitignore --fix .",
    "pretest": "npm run lint",
    "start": "phenomic start",
    "test": "npm run build"
  },
  "phenomic": {
    "CNAME": true,
    "cache": true,
    "offline": true
  },
  "#babel": "webpack-(development|production) are useful for webpack 2, otherwise use development|production",
  "babel": {
    "env": {
      "development": {
        "plugins": [
          "react-hot-loader/babel"
        ],
        "presets": [
          "babel-preset-latest",
          "babel-preset-react",
          "babel-preset-stage-1"
        ]
      },
      "production": {
        "presets": [
          "babel-preset-latest",
          "babel-preset-react",
          "babel-preset-react-optimize",
          "babel-preset-stage-1"
        ]
      },
      "webpack-development": {
        "plugins": [
          "react-hot-loader/babel"
        ],
        "presets": [
          [
            "babel-preset-latest",
            {
              "es2015": {
                "modules": false
              }
            }
          ],
          "babel-preset-react",
          "babel-preset-stage-1"
        ]
      },
      "webpack-production": {
        "presets": [
          [
            "babel-preset-latest",
            {
              "es2015": {
                "modules": false
              }
            }
          ],
          "babel-preset-react",
          "babel-preset-react-optimize",
          "babel-preset-stage-1"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-i-am-meticulous/react"
    ],
    "parser": "babel-eslint"
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "dependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.5",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-1": "^6.13.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.7.1",
    "eslint-config-i-am-meticulous": "^4.1.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "history": "^2.0.0",
    "invariant": "^2.1.1",
    "json-loader": "^0.5.2",
    "moment": "^2.15.1",
    "npm-run-all": "^1.7.0",
    "phenomic": "^0.17.12",
    "postcss-browser-reporter": "^0.4.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-loader": "^1.0.0",
    "postcss-reporter": "^1.3.0",
    "raw-loader": "^0.5.1",
    "react": "^0.14.0 || ^15.0.0-rc.1",
    "react-dom": "^0.14.0 || ^15.0.0-rc.1",
    "react-google-analytics": "^0.2.0",
    "react-helmet": "^3.0.0",
    "react-hot-loader": "^3.0.0-beta",
    "react-icons": "^2.2.1",
    "react-redux": "^4.0.0",
    "react-router": "^3.0.0",
    "react-social-sharebuttons": "trkw/react-social-sharebuttons",
    "react-svg-inline": "^1.1.0",
    "react-topbar-progress-indicator": "^1.0.0",
    "redux": "^3.0.0",
    "style-loader": "^0.13.0",
    "stylelint": "^7.2.0",
    "stylelint-config-standard": "^14.0.0",
    "webpack": "^1.11.0 || ^2.1.0-beta.25",
    "whatwg-fetch": "^0.11.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^4.0.0"
  }
}
